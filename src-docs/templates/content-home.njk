<!DOCTYPE html>
<html lang="en-us">
  <head>
    {% include "./head.njk" %}
    <style>
      main h3 {
        font-size: 125%;
      }
      #anatomy {
        margin-top: 2em;
        margin-bottom: 1em;
      }
      figure img {
        width: 90%;
        border: 2px solid #888;
      }
      figcaption {
        font-size: 150%;
        text-align: center;
        font-weight: bold;
      }
    </style>
  </head>
  <body>
    {% include "./header.njk" %}

    <!-- Main section-->
    <div id="main-section" class="container px-4">
      <nav aria-label="Breadcrumb">
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="index.html">Home</a></li>
        </ol>
      </nav>
      <main class="container px-4">
        <div class="row gx-4 justify-content-center">
          <div class="col-lg-10">

            {% block content %}

              <h1>{{title}}</h1>




             {% include "./feedback.njk" %}

            {% endblock %}
          </div>
        </div>
      </main>
    </div>
    {% include "./footer.njk" %}
    {% include "./scripts.njk" %}

  </body>
</html>
