<!DOCTYPE html>
<html lang="en-us">
  <head>
    {% include "./head.njk" %}
  </head>
  <body>
    {% include "./header.njk" %}

    <!-- Main section-->
    <div id="main-section" class="container px-4">
      <nav aria-label="Breadcrumb">
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="index.html">Home</a></li>
          {% if breadcrumb %}
            <li class="breadcrumb-item">{{breadcrumb}}</li>
          {% endif %}
        </ol>
      </nav>
      <main class="container px-4">
        <div class="row gx-4 justify-content-center">
          <div class="col-lg-10">

            {% block content %}

              <h1>{{title}}</h1>

                {% include './' + content %}

             {% include "./feedback.njk" %}

            {% endblock %}
          </div>
        </div>
      </main>
    </div>
    {% include "./footer.njk" %}
    {% include "./scripts.njk" %}

  </body>
</html>
